   <children>
      <ButtonBar buttonMinWidth="85.0" nodeOrientation="RIGHT_TO_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <buttons>
            <Button fx:id="buttonEmployees" mnemonicParsing="false" onAction="#editEmployees" text="Employees" />
            <Button fx:id="buttonDepartments" mnemonicParsing="false" onAction="#editDepartments" text="Departments" />
        </buttons>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ButtonBar>
   </children>
</AnchorPane>
