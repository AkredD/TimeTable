<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tableGUI.view.mainDepartmentTableView.departmentTableView.TableOverviewController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <TableView fx:id="personTable" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="nameID" maxWidth="60.0" prefWidth="60.0" text="Name" />
                <TableColumn fx:id="positionID" maxWidth="60.0" prefWidth="60.0" text="Position" />
                  <TableColumn fx:id="idID" maxWidth="60.0" prefWidth="60.0" text="ID" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
